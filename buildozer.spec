[app]
# (str) Title of your application
title = Racha

# (str) Package name
package.name = yourapptitle

# (str) Package domain (reverse DNS style)
package.domain = org.example

# (list) Application requirements
requirements = python3,kivy,kivymd,reportlab

# (str) Application icon
icon.filename = icon.png

# (str) Name of the application executable
filename = Racha

# (str) Version of the application
version = 0.1

# (str) Application source code directory (you can put the source code in the root directory, so source.dir is ".")
source.dir = .
